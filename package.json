{
  "name": "disease-classification",
  "version": "0.0.1",
  "main": "main.js",
  "repository": "https://github.com/Stevenah/disease-classification.git",
  "author": "Stevenah <shicks1000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --env=development --progress --profile --colors",
    "build": "webpack --env=development",
    "test": "jest .test.js",
    "test-watch": "jest .test.js --watch",
    "watch": "webpack --env=development --watch",
    "build-prod": "webpack -p --env=production"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "^layout(.*)$": "<rootDir>/webapp/components/layout/$1",
      "^pages(.*)$": "<rootDir>/webapp/components/pages/$1",
      "^components(.*)$": "<rootDir>/webapp/components/$1",
      "^reducers(.*)$": "<rootDir>/webapp/reducers$1",
      "^actionConstants(.*)$": "<rootDir>/webapp/actionConstants",
      "^actions(.*)$": "<rootDir>/webapp/actions",
      "^utils(.*)$": "<rootDir>/webapp/utils",
      "^store(.*)$": "<rootDir>/webapp/store",
      "^epics(.*)$": "<rootDir>/webapp/epics",
      "^api(.*)$": "<rootDir>/webapp/api",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/webapp/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/tests/webapp/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/tests/webapp/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-env": "^2.4.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "compass": "^0.1.1",
    "compass-mixins": "^0.12.10",
    "css-loader": "^0.28.4",
    "d3": "^4.11.0",
    "d3-array": "^1.2.1",
    "d3-scale": "^1.0.6",
    "d3-shape": "^1.2.0",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^1.6.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "express": "^4.16.2",
    "file-loader": "^0.11.2",
    "fine-uploader": "^5.15.6",
    "fine-uploader-wrappers": "^1.0.1",
    "html-loader": "^0.5.0",
    "immutability-helper": "^2.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^22.4.2",
    "jsdom": "^11.6.2",
    "lodash": "^4.17.4",
    "mixin-loader": "^2.0.3",
    "nock": "^9.2.3",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "react": "16.0.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-bootstrap": "^0.31.2",
    "react-d3-basic": "^1.6.11",
    "react-d3-core": "^1.3.9",
    "react-dom": "16.0.0",
    "react-dropzone": "^4.2.1",
    "react-fine-uploader": "^1.0.9",
    "react-fontawesome": "^1.6.1",
    "react-grid-gallery": "^0.4.2",
    "react-highcharts": "^13.0.0",
    "react-loader": "^2.4.2",
    "react-modal": "^2.2.4",
    "react-modal-dialog": "^4.0.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "react-test-renderer": "^16.2.0",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "redux-observable": "^0.16.0",
    "redux-router": "^2.1.2",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.3",
